
<div class="container">
	<div class="row">
		<div class="col-md-12">
			<p id="notice"><%= notice %></p>
	
			<p>
			  <strong>Name:</strong>
			  <%= @patient.name %>
			</p>

			<p>
			  <strong>Phone:</strong>
			  <%= @patient.phone %>
			</p>
			<%= link_to 'Edit', edit_patient_path(@patient), class: 'btn btn-info' %> |
			<%= link_to 'Back', patients_path, class: 'btn btn-secondary' %>
		</div>
	</div>
</div>
